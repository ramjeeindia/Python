    "def summation(arr):\n",
    "    n=len(arr)\n",
    "    sumval=0\n",
    "    for i in range(n):\n",
    "        sumval+=arr[i]\n",
    "    return sumval\n",
    "\n",
    "arr = [23,34,64,12,25,56,68]\n",
    "result = summation(arr)\n",
    "print(result)"
